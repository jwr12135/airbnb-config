{
  "extends": [
    "plugin:@typescript-eslint/recommended",
    "airbnb",
    "airbnb-typescript",
    "airbnb/hooks",
    "@airbnb-config/base"
  ],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "project": "tsconfig.json"
  },
  "rules": {
    "@typescript-eslint/naming-convention": "off",
    "react/prop-types": "off",
    "react/react-in-jsx-scope": "off"
  }
}
