{
  "extends": ["plugin:@typescript-eslint/recommended", "airbnb-typescript", "@airbnb-config/base"],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "project": "tsconfig.json"
  },
  "rules": {
    "@typescript-eslint/naming-convention": ["error", { "format": ["camelCase", "PascalCase"] }],
    "react/react-in-jsx-scope": "off"
  }
}
